#include<bits/stdc++.h>
using namespace std;

int leader(int arr[],int n)
{
    cout<<arr[n-1]<<" ";
    int right_max=arr[n-1];
    for(int i=n-2;i>=0;i--)
    {
        if(arr[i]>right_max)
        {
            cout<<arr[i]<<" ";
            right_max=arr[i];
        }
    }
}
int main()
{
    int arr[]={1,2,3,4,5,277,4,5,2,55};
    int n=sizeof(arr)/sizeof(arr[0]);
    leader(arr,n);
    return 0;
}
